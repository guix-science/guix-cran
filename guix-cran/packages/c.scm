    (source
     (origin
       (method url-fetch)
       (uri (cran-uri "cytometree" version))
       (sha256
        (base32 "18g7av73lmnyga1kk24bf8jy599zn9n6qhr13mxsqgi0zdinicfa"))))
    (properties `((upstream-name . "cytometree")))
    (build-system r-build-system)
    (propagated-inputs (list r-rcpparmadillo
